
	 <%- include('header') %>
	 <div id="" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
              <div class="panel panel-info">
                   <div class="panel-heading">
                      <div class="panel-title"> 
                            <h4 class="text-center"><b>View All Roles</b></h4>
                      </div>
                   </div> 
        <table class="table table-striped table-primary">
          <thead>
            <tr>
              <th style="padding-left: 25%">Role</th>
              <th style="float: right; color: blue">
              	<a href="/addRole"><b>Add New Role</b></a>
              </th>
            </tr>
          </thead>
          <tbody> 
                  <% for(var i in get_role) {%>
               <tr>
                  <td style="padding-left: 25%"><%= get_role[i].Role %></td>
                  <td style="padding-left: 25%">
                     <a class="btn btn-danger btn-xs" href="viewRole/delete/<%= get_role[i]._id %>" 
                      	 onclick="return confirm('Are you sure?')">
                      	<i class="fa fa-trash"></i>
                    </a>
                  </td>
               </tr>
                <% } %> 
           </tbody>
          </table>
              </div>
      </div>        
                    